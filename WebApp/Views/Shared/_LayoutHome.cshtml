<html class="no-js">
<!--<![endif]-->
<head>
    @Html.Partial("_LayoutHead")
    @RenderSection("description", required: false)
</head>
<body>
    @Html.Partial("_Header")
 
    <div class="main-wrapper">

        
        
       
        <section class="content-wrapper">
         
          @Html.Partial("_Banner")
      
            @Html.Action("SideBar", "Home")
            <main class="main-content ">
            
                @Html.Partial("_ModalStructure")
                @RenderBody()
                @Html.Partial("_Loading")
            </main>
        </section>
        @Html.Partial("_Footer")
    </div>
    @Scripts.Render("~/Scripts/JQuery/jquery")
    @Scripts.Render("~/Scripts/Views/Shared/shared")
    @RenderSection("scripts", required: false)
    @Html.Partial("_ModalLogin")
    @Html.Partial("_ModalMessage")
</body>
@RenderSection("end", required: false)
</html>