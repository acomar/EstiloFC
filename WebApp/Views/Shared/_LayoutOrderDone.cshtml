<html class="no-js">
 <!--<![endif]-->
<head>
    <title>@Infrastructure.Resources.HomeResource.TagTitle</title>
    @Html.Partial("_LayoutHead")
</head>
<body>
    <div class="main-wrapper">
        @Html.Partial("_Header")
        <section class="content-wrapper">
            <main class="full-content pedido-finalizado">
                @Html.Partial("_ModalStructure")
                @RenderBody()
                @Html.Partial("_Loading")
            </main>
        </section>
        @Html.Partial("_Footer")
    </div>
    @Scripts.Render("~/Scripts/Shared/shared")

    @RenderSection("scripts", required: false)

    @Html.Partial("_ModalLogin")
    @Html.Partial("_ModalMessage")
</body>
</html>